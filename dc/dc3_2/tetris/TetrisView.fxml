<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<HBox xmlns="http://javafx.com/javafx/8.0.162"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="dc3_2.tetris.TetrisController">
	<children>
		<VBox maxHeight="-Infinity" maxWidth="-Infinity"
			minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
			prefWidth="200.0">
			<children>
				<Canvas fx:id="tetrisCanvas" height="400.0" width="200.0" />
			</children>
		</VBox>
		<VBox prefHeight="400.0" prefWidth="101.0">
			<children>
				<Canvas fx:id="nextTetriminoCanvas" height="100.0"
					width="99.0" />
				<Canvas fx:id="tetriminoAfterNextCanvas" height="100.0"
					width="100.0" />
			</children>
		</VBox>
	</children>
</HBox>
